{
  "semi":  true,
  "singleQuote":  true,
  "printWidth":  120,
  "useTabs": false,
  "tabWidth":  2,
  "bracketSpacing": true,
  "endOfLine":"lf",
  "importOrder": [
    "^[./]",
    "^@app/(.*)$",
    "^@ui/(.*)$",
    ".css$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["prettier-plugin-tailwindcss"]

}
